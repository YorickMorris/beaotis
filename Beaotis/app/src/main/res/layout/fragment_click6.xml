<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".findingobjects.fruits.groupone.Click6">

    <ImageView
        android:id="@+id/imageViewFruitsArmut3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/armut"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewFruitsAnanas1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginHorizontal="30dp"/>

    <TextView
        android:id="@+id/elmaTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elma"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsElma3"
        app:layout_constraintEnd_toStartOf="@id/ananasTextView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="30dp"/>

    <TextView
        android:id="@+id/armutTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Armut"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsArmut3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@id/ananasTextView2"
        android:layout_marginHorizontal="30dp"
        />

    <ImageView
        android:id="@+id/imageViewFruitsElma3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/elma"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/imageViewFruitsAnanas1"
        android:layout_marginHorizontal="30dp" />

    <ImageView
        android:id="@+id/imageViewFruitsAnanas1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ananas"
        app:layout_constraintEnd_toStartOf="@id/imageViewFruitsArmut3"
        app:layout_constraintStart_toEndOf="@id/imageViewFruitsElma3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginHorizontal="30dp"/>

    <TextView
        android:id="@+id/ananasTextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ananas"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@id/armutTextView3"
        app:layout_constraintStart_toEndOf="@id/elmaTextView3"
        app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsAnanas1"
        android:layout_marginHorizontal="30dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>