<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".findingobjects.fruits.groupone.Click7">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:weightSum="3"
        android:orientation="horizontal">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <TextView
        android:id="@+id/elmaTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Elma"
        android:textSize="30sp"
        android:textStyle="bold"
        android:layout_gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsElma4"
        app:layout_constraintEnd_toStartOf="@id/ananasTextView3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginHorizontal="10dp"/>
        <ImageView
            android:id="@+id/imageViewFruitsElma4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/elma"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@id/imageViewFruitsAnanas2"
            android:layout_marginHorizontal="10dp" />
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/ananasTextView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ananas"
            android:textSize="30sp"
            android:layout_gravity="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@id/armutTextView4"
            app:layout_constraintStart_toEndOf="@id/elmaTextView4"
            app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsAnanas2"
            android:layout_marginHorizontal="10dp"
            />

        <ImageView
            android:id="@+id/imageViewFruitsAnanas2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/ananas"
            app:layout_constraintEnd_toStartOf="@id/imageViewFruitsArmut4"
            app:layout_constraintStart_toEndOf="@id/imageViewFruitsElma4"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginHorizontal="10dp"/>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <TextView
        android:id="@+id/armutTextView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Armut"
        android:textSize="30sp"
        android:layout_gravity="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imageViewFruitsArmut4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        android:layout_marginHorizontal="10dp"
        app:layout_constraintStart_toEndOf="@id/ananasTextView3"
        />
        <ImageView
            android:id="@+id/imageViewFruitsArmut4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/armut"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginHorizontal="10dp"
            app:layout_constraintStart_toEndOf="@+id/imageViewFruitsAnanas2"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.appcompat.widget.LinearLayoutCompat>


    </androidx.appcompat.widget.LinearLayoutCompat>
</androidx.constraintlayout.widget.ConstraintLayout>